func fibonacci(n){
    checkif(n == 0){
        throw 0;
    } recheck(n==1) {
        throw 1;
    } otherwise {
        throw fibonacci(n-1) + fibonacci(n-2);
    }
}

var n = integer(get("Enter n: "));
var fib_list = [];

each (var i = 0; i < n; i++) {
    fib_list += [fibonacci(i)];
}


show(reverse(fib_list));