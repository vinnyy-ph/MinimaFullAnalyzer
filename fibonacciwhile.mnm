var i = 0, n;
var a = 0, b = 1, c;
var sum_fib = a + b; 

n = integer(get("enter n: "));

show(a + " ");
checkif (n > 1) {
    show(b + " ");
}

repeat (i <= n) {
    c = a + b;
    show(c + " ");
    sum_fib = sum_fib + c;
    a = b;
    b = c;
    i++;
}

show("\nSum of Fibonacci numbers: " + sum_fib);